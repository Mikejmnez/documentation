   &lt;response reqID="####"&gt;
       &lt;showServices&gt;
           &lt;service name="DAP"&gt;
               &lt;command name="ddx"&gt;
                   &lt;description&gt;Words For Humans&lt;/description&gt;
                   &lt;format name="dap2"/&gt;
               &lt;/command&gt;
               &lt;command name="dds"&gt;
                   &lt;description&gt;Words For Humans&lt;/description&gt;
                   &lt;format name="dap2"/&gt;
               &lt;/command&gt;
           &lt;/service&gt;
       &lt;/showServices&gt;
   &lt;/response&gt;
</pre>



